{
  "name": "fastify-schema-to-typescript",
  "version": "5.0.0",
  "description": "Convert json schema for Fastify to typescript interface",
  "main": "./dist/index.js",
  "bin": {
    "fastify-schema-to-typescript": "./dist/src/cli.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasthiebaud/fastify-schema-to-typescript.git"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasthiebaud/fastify-schema-to-typescript/issues"
  },
  "homepage": "https://github.com/thomasthiebaud/fastify-schema-to-typescript#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.8",
    "fastify": "^3.25.3",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^10.1.5"
  },
  "peerDependencies": {
    "fastify": "^3.21.6"
  }
}
