{
  "name": "fastify-schema-to-typescript",
  "version": "4.0.0",
  "description": "Convert json schema for Fastify to typescript interface",
  "main": "./dist/index.js",
  "bin": {
    "fastify-schema-to-typescript": "./dist/src/cli.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasthiebaud/fastify-schema-to-typescript.git"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasthiebaud/fastify-schema-to-typescript/issues"
  },
  "homepage": "https://github.com/thomasthiebaud/fastify-schema-to-typescript#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^16.4.13",
    "fastify": "3.20.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "glob": "^7.1.7",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^10.1.4"
  },
  "peerDependencies": {
    "fastify": "^3.20.1"
  }
}
